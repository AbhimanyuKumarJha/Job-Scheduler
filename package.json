{
  "name": "job-scheduler",
  "version": "1.0.0",
  "description": "Production-ready job scheduler with cron-based scheduling, retry policies, and comprehensive logging",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:clear": "ts-node src/scripts/clear.ts",
    "db:stats": "ts-node src/scripts/stats.ts",
    "lint": "echo \"No linter configured yet\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent"
  },
  "keywords": [
    "job-scheduler",
    "cron",
    "task-scheduler",
    "job-queue",
    "background-jobs",
    "typescript",
    "nodejs",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.2",
    "cron-parser": "^5.4.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "node-cron": "^4.2.1",
    "openapi-types": "^12.1.3",
    "pg": "^8.16.3",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.8",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
